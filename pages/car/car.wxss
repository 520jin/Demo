.head{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.lefthead{
  display: flex;
}
.righthead{
  margin-right: 20rpx;
  color: #bbb;
}
.backfont{
  line-height: 2.5;
}
.car {
  height: 560px;
  font-size: 16px;
  color: #ccc;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.car image {
  width: 104rpx;
  height: 101rpx;
 
}
.carItem{
  display: flex;
  margin: 20rpx;

}
.carItem image{
  width: 200rpx;
  height: 200rpx;

}
.detail{
  display: flex;
  flex-direction: column;
  font-size: 16px;
  margin-left: 10rpx;
 
}
.tip{
  color:#bbb;
  font-size: 14px;
}
.price{
  color: #f00;
}
.addresss{
  margin: 20rpx;
  font-size: 16px;
  border-bottom: 1rpx solid rgba(0,0,0,.1);
  padding-bottom: 20rpx;
}
.addresss image{
  width: 64rpx;
  height: 65rpx;
}